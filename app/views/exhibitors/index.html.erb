<%= render 'components/expo_banner', expo: @expo %>
<div class="other-page-content">
  <%= render 'components/expo-tabs' %>
  <div class="exhibitor-item-render exhibitor-list toggle-visibility">
    <div class="search light-paragraph">
      <%= form_tag expo_exhibitors_path(@expo), method: :get, remote: true do %>
      <%= text_field_tag :query,
      params[:query],
      class: "form-control",
        placeholder: "Find exhibitors", autocomplete:"off"
        %>
        <% end %>
    </div>

    <div id="list-item-render">
      <%= render 'components/exhibitor_list_item_render', exhibitors: @exhibitors %>
    </div>
  </div>

  <div class="exhibitor-item-render favorite-list ">
    <%= render 'components/user_favorites_render' %>
  </div>
</div>
<div class="flash-message-box">
  <div class="flash-message">
  </div>
</div>

<%= javascript_pack_tag 'infinite_scroll' %>
